{
  "name": "MyZoo",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$ignoreUnknownInstances": true,
      "$path": "src/shared",
      "Remotes": {
        "$className": "Folder",
        "GetData": { "$className": "RemoteFunction" },
        "GetAllData": { "$className": "RemoteFunction" },
        "UpdateCash": { "$className": "RemoteEvent" }
      }
    },

    "ServerScriptService": {
      "$path": "src/server",
      "Packages": { "$path": "ServerPackages" }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client",
        "$ignoreUnknownInstances": true
      }
    }
  }
}
