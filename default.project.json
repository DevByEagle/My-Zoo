{
  "name": "MyZoo",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/shared",
      "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "Game": { "$path": "src/server" }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": { "$path": "src/client" }
      }
    },

    "Workspace": {
      "Plots": {
        "$className": "Folder"
      }
    }
  }
}
